{
  "name": "pack.js",
  "version": "0.1.0",
  "description": "Makes html page minified by packing all depended css and js in one file",
  "preferGlobal": "true",
  "bin": {
    "packjs": "./bin/packjs"
  },
  "scripts": {
    "start": "node ./bin/packjs --help",
    "test": "node ./bin/packjs -i tests/index.html -o tests/out"
  },
  "main": "./lib/pack.js",
  "repository": "git://github.com/megatolya/packjs.git",
  "dependencies": {
    "mkdirp": "0.3.5",
    "commander": "0.3.x",
    "cheerio": "~0.10.8",
    "async": "~0.2.6",
    "request": "~2.16.2",
    "borschik": "0.2.8",
    "node-uuid": "1.4.0",
    "wrench": "~1.4.4",
    "inherit": "~1.0.4",
    "colors": "0.6.x"
  },
  "keywords": [
    "pack",
    "html",
    "minify",
    "one",
    "file"
  ],
  "author": "Anatoliy Ostrovskiy",
  "license": "MIT",
  "engines": {
     "node": "0.8.x"
   }
}
